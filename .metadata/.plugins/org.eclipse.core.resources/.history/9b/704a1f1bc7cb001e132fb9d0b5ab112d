package sec10;

public class Ex01 {

	public static void main(String[] args) {

		 int[][] array = { 
				 	{95, 86},
				 	{83, 92, 96},
				 	{78, 83, 93, 87, 88}
				  	};

	
			int sum = 0;
			for(int [] i : array) {
				for(int score: i) {
					sum += score;
				}
			}

			System.out.println("Á¡¼ö ÃÑÇÕ : " + sum);
	}

}
