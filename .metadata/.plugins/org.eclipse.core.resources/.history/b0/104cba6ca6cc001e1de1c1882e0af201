package hw;

import java.util.Scanner;

public class CalEx {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		Cal c1 = new Cal();
		
		c1.powerOn();
		
		int r1 = c1.add(5, 8);
		System.out.println("오챯叩 : " + r1);
		
		int r2 = c1.min(5, 8);
		System.out.println("짋챯叩 : " + r2);
		
		double r3 = c1.cro(5, 8);
		System.out.println("썼챯叩 : " + r3);
		
		int x = 10, y = 30 ;
		double r4 = c1.divide(x, y);
		System.out.println("씱얋 써쎀 : " +r4);
		
		c1.powerOff();
	}

}
