package sec07;

public class MultiArrayCreateEx01 {

	public static void main(String[] args) {


		//2차원 배열 생성 = [][]
		int[][] scores = {
				{15, 25, 35},
				{50, 70, 30, 40, 90}
		};
		
		//배열의 길이
		System.out.println(scores.length);
		System.out.println(scores[0].length);
		System.out.println(scores[1].length);
		
		//첫 번째 반의 세번째 학생 점수는?
		//두 번재 반의 두 번째 학생 점수는?
		//첫 번째 반의 평균 점수는?
		//두 번째 반의 평균 점수는?
		
		
	}

}
